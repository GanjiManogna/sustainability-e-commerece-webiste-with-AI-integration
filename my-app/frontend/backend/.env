# backend/.env

# Django Settings
DJANGO_SECRET_KEY='your_strong_random_secret_key_here' # Generate a strong key later
DJANGO_DEBUG=True # Set to False in production

# PostgreSQL Database
POSTGRES_DB=ecorevive_db  # Choose a name for your database
POSTGRES_USER=your_db_user # Your PostgreSQL username
POSTGRES_PASSWORD=your_db_password # Your PostgreSQL password
POSTGRES_HOST=localhost # Or the IP/hostname if your DB is remote
POSTGRES_PORT=5432 # Default PostgreSQL port